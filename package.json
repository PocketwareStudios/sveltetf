{
  "name": "sveltetf",
  "version": "0.0.13",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eksohinc/sveltetf.git"
  },
  "author": "Fran√ßois Paquette",
  "bugs": {
    "url": "https://github.com/eksohinc/sveltetf/issues"
  },
  "keywords": [
    "svelte",
    "sveltetf",
    "TensorflowJS",
    "ml"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "test": "playwright test",
    "vitest": "vitest run src",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "@mediapipe/tasks-vision": "^0.10.8",
    "@playwright/test": "^1.40.1",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.27.7",
    "@sveltejs/package": "^2.2.3",
    "@tensorflow/tfjs-automl": "^1.3.0",
    "@tensorflow/tfjs-backend-webgl": "^4.14.0",
    "@tensorflow/tfjs-converter": "^4.14.0",
    "@tensorflow/tfjs-core": "^4.14.0",
    "@tensorflow-models/body-pix": "^2.2.1",
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow-models/face-detection": "^1.0.2",
    "@tensorflow-models/face-landmarks-detection": "^1.0.5",
    "@tensorflow-models/handpose": "^0.1.0",
    "@tensorflow-models/mobilenet": "^2.1.1",
    "@tensorflow-models/posenet": "^2.2.2",
    "@tensorflow-models/qna": "^1.0.2",
    "@tensorflow-models/toxicity": "^1.2.2",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/svelte": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "scatter-gl": "^0.0.13",
    "svelte-check": "^3.6.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "peerDependencies": {
    "svelte": "^4.2.8"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "description": "## this is a typescript version of the library built by John Kane: https://github.com/john--kane/svelteml",
  "homepage": "https://github.com/eksohinc/sveltetf#readme",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "eslint-compat-utils": "^0.1.2",
    "eslint-scope": "^7.2.2",
    "eslint-visitor-keys": "^3.4.3",
    "svelte": "^4.2.8",
    "svelte-eslint-parser": "^0.33.1"
  },
  "types": "./dist/index.d.ts"
}
