{
	"name": "sveltetf",
	"version": "0.0.12",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/eksohinc/sveltetf.git"
	},
	"author": "Fran√ßois Paquette",
	"bugs": {
		"url": "https://github.com/eksohinc/sveltetf/issues"
	},
	"keywords": [
		"svelte",
		"sveltetf",
		"TensorflowJS",
		"ml"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"preview": "vite preview",
		"test": "playwright test",
		"vitest": "vitest run src",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"dependencies": {
		"@tensorflow-models/body-pix": "^2.2.0",
		"@tensorflow-models/coco-ssd": "^2.2.2",
		"@tensorflow-models/face-landmarks-detection": "^1.0.2",
		"@tensorflow-models/handpose": "^0.0.7",
		"@tensorflow-models/knn-classifier": "^1.2.4",
		"@tensorflow-models/mobilenet": "^2.1.0",
		"@tensorflow-models/posenet": "^2.2.2",
		"@tensorflow-models/qna": "^1.0.1",
		"@tensorflow-models/toxicity": "^1.2.2",
		"@tensorflow-models/universal-sentence-encoder": "^1.3.3",
		"@tensorflow/tfjs-automl": "^1.3.0",
		"@tensorflow/tfjs-backend-webgl": "^3.21.0",
		"@tensorflow/tfjs-converter": "^3.21.0",
		"@tensorflow/tfjs-core": "^3.21.0",
		"scatter-gl": "^0.0.13"
	},
	"devDependencies": {
		"@playwright/test": "^1.32.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.2",
		"@sveltejs/package": "^1.0.2",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte-check": "^3.2.0",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.2.1",
		"vitest": "^0.25.8"
	},
	"peerDependencies": {
		"svelte": "^3.58.0"
	},
	"type": "module"
}
